{
  "name": "jalgoarena",
  "version": "1.0.0",
  "description": "JAlgoArena UI",
  "main": "server.js",
  "scripts": {
    "installRequiredGlobalDependencies": "npm install webpack -g & npm install pm2 -g",
    "test": "echo 'Tests not implemented yet'",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node server.js",
    "prepm2start": "npm run build",
    "pm2start": "NODE_ENV=production pm2 start server.js --name jalgoarena-ui",
    "preherokuStart": "npm run herokuBuild",
    "herokuStart": "NODE_ENV=production node server.js",
    "dev": "node server.js --trace-sync-io",
    "prod": "NODE_ENV=production node server.js",
    "prebuild": "node deletePublicDir.js",
    "build": "NODE_ENV=production webpack --progress --config webpack.config.prod.js",
    "herokuBuild": "NODE_ENV=production ./node_modules/.bin/webpack --progress --config webpack.config.prod.js",
    "predeploy": "npm run build",
    "deploy": "node createReleasePackage.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spolnik/JAlgoArena-UI.git"
  },
  "keywords": [
    "JAlgoArena"
  ],
  "author": "Jacek Sp√≥lnik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spolnik/JAlgoArena-UI/issues"
  },
  "homepage": "https://github.com/spolnik/JAlgoArena-UI#readme",
